<h5>Bulk Upload Success Results</h5>


<% @sheet_results.each do |sheet, info| %>
  <% rows = info[:saved_rows] %>
  <% if !rows.empty? %>

  <h6 ><%="Sheet: #{sheet} - #{rows.size} Saved Rows"%></h6>
  <table class="table table-bordered table-sm table-font-sm">

    <thead class="thead-light thead-font-sm">
      <tr>
        <% info[:header].each do |col| %>
          <th><%= col %></th>
        <% end %>
      </tr>
    </thead>

    <tbody>
    <% rows.each do |row|%>
      <tr>
        <% row.each do |value| %>
          <td><%= value.to_s %></td>
        <% end %>
      </tr>
    <% end %>
    </tbody>

  </table>
  <% end %>
<% end %>


<h5>Editing dissected sample: <%=h @sample.barcode_key %></h5>

<%= form_for @sample, url: {controller: 'dissected_samples', action: :update}, remote: true do |f| %>
    <%= render '/shared/errors', model: @sample %>
  
  <%= render :partial => 'dsample_form', :locals => {:f => f, :dissect => @sample} %> 
  <%=raw break_clear(f.submit "Update", class: "btn btn-primary") %>
<% end %>
